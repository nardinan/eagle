<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE transformations PUBLIC "-//NetBeans//DTD Module Automatic Dependencies 1.0//EN" "http://www.netbeans.org/dtds/module-auto-deps-1_0.dtd">
<transformations version="1.0">
    <transformationgroup>
        <description>libs.jzlib and libs.jsch renamed</description>
        <transformation>
            <trigger-dependency type="cancel">
                <module-dependency codenamebase="org.netbeans.libs.jzlib"/>
            </trigger-dependency>
            <implies>
                <result>
                    <module-dependency codenamebase="com.jcraft.jzlib" spec="1.0.7"/>
                </result>
            </implies>
        </transformation>
        <transformation>
            <trigger-dependency type="cancel">
                <module-dependency codenamebase="org.netbeans.libs.jsch"/>
            </trigger-dependency>
            <implies>
                <result>
                    <module-dependency codenamebase="com.jcraft.jsch" spec="0.1.43"/>
                </result>
            </implies>
        </transformation>
    </transformationgroup>
    <transformationgroup>
        <description>libs.commons_codec and libs.commons_logging renamed</description>
        <transformation>
            <trigger-dependency type="cancel">
                <module-dependency codenamebase="org.netbeans.libs.commons_codec"/>
            </trigger-dependency>
            <implies>
                <result>
                    <module-dependency codenamebase="org.apache.commons.codec" spec="1.3.0"/>
                </result>
            </implies>
        </transformation>
        <transformation>
            <trigger-dependency type="cancel">
                <module-dependency codenamebase="org.netbeans.libs.commons_logging"/>
            </trigger-dependency>
            <implies>
                <result>
                    <module-dependency codenamebase="org.apache.commons.logging" spec="1.1.0"/>
                </result>
            </implies>
        </transformation>
    </transformationgroup>    
    <transformationgroup>
        <description>libs.springframework replaced by org.springframework.* bundles</description>
        <transformation>
            <trigger-dependency type="cancel">
                <module-dependency codenamebase="org.netbeans.libs.springframework"/>
            </trigger-dependency>
            <implies>
                <result>
                    <module-dependency codenamebase="org.springframework.beans" spec="3.1.1"/>
                </result>
                <result>
                    <module-dependency codenamebase="org.springframework.context" spec="3.1.1"/>
                </result>
                <result>
                    <module-dependency codenamebase="org.springframework.core" spec="3.1.1"/>
                </result>
            </implies>
        </transformation>
    </transformationgroup>    
</transformations>
